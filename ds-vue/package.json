    "postcss": "8.3.0",
    "postcss-loader": "^4.2",
    "storybook-builder-vite": "^0.1.16",
    "storybook-dark-mode": "^1.0.9",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vite": "^2.5.2",
    "vite-plugin-eslint": "^1.3.0",
